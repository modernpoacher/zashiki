{
  "name": "@modernpoacher/zashiki",
  "version": "1.1.16",
  "description": "Zashiki",
  "main": "./index.js",
  "author": "Jonathan Perry for Modern Poacher Limited <modernpoacher@modernpoacher.com>",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=16.13.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com/modernpoacher/zashiki.git"
  },
  "scripts": {
    "prepublishOnly": "cross-env NODE_ENV=production npm run babel",
    "prebuild": "npm run clean",
    "start": "cross-env NODE_ENV=production npm run build",
    "dev": "cross-env NODE_ENV=development npm run build",
    "build": "npm run babel",
    "build:watch": "cross-env NODE_ENV=development npm run babel:watch",
    "clean": "rimraf lib",
    "babel": "babel src -d lib",
    "babel:watch": "npm run babel -- -w",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "cross-env NODE_ENV=test mocha test --recursive -r @babel/register --verbose",
    "pre-commit": "npm run lint && npm test && npm run build && git add lib"
  },
  "dependencies": {
    "@babel/runtime": "^7.21.0",
    "@hapi/boom": "^10.0.1",
    "@hapi/joi": "^17.1.1",
    "@modernpoacher/zashiki-transport": "2.0.14",
    "concurrently": "^8.0.1",
    "debug": "^4.3.4",
    "dotenv": "^16.0.3",
    "nconf": "^0.12.0",
    "shinkansen-rails": "1.0.18",
    "shinkansen-signals": "1.1.2",
    "shinkansen-transmission": "2.2.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.3",
    "@babel/eslint-parser": "^7.21.3",
    "@babel/plugin-proposal-export-default-from": "^7.18.10",
    "@babel/plugin-transform-runtime": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/register": "^7.21.0",
    "babel-loader": "^9.1.2",
    "babel-plugin-module-resolver": "^5.0.0",
    "chai": "^4.3.7",
    "core-js": "^3.29.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.37.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "mocha": "^10.2.0",
    "rimraf": "^4.4.1"
  }
