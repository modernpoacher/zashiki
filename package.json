{
  "name": "@modernpoacher/zashiki",
  "version": "1.1.16",
  "description": "Zashiki",
  "main": "./index.js",
  "author": "Jonathan Perry for Modern Poacher Limited <modernpoacher@modernpoacher.com>",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=12.19.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com/modernpoacher/zashiki.git"
  },
  "scripts": {
    "prepublishOnly": "cross-env NODE_ENV=production npm run babel",
    "prebuild": "npm run clean",
    "start": "cross-env NODE_ENV=production npm run build",
    "dev": "cross-env NODE_ENV=development npm run build",
    "build": "cross-env NODE_ENV=development npm run babel",
    "build:watch": "cross-env NODE_ENV=development npm run babel:watch",
    "clean": "rimraf lib",
    "babel": "babel src -d lib",
    "babel:watch": "npm run babel -- -w",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "NODE_ENV=test mocha test --recursive -r @babel/register --verbose",
    "pre-commit": "npm run lint && npm test && npm run build && git add lib"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@hapi/boom": "^9.1.0",
    "@hapi/joi": "^17.1.1",
    "@modernpoacher/zashiki-transport": "2.0.14",
    "concurrently": "^5.3.0",
    "debug": "^4.3.1",
    "dotenv": "^8.2.0",
    "nconf": "^0.11.0",
    "shinkansen-rails": "1.0.17",
    "shinkansen-signals": "1.0.17",
    "shinkansen-transmission": "2.1.16"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/plugin-proposal-export-default-from": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "@babel/register": "^7.12.10",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.0.0",
    "chai": "^4.2.0",
    "core-js": "^3.8.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.15.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-import-resolver-babel-module": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "husky": "^4.3.6",
    "mocha": "^8.2.1",
    "rimraf": "^3.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  }
}
